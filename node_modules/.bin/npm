#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/marcosm/code/life/life-server/node_modules/.pnpm/npm@9.6.5/node_modules/npm/bin/node_modules:/Users/marcosm/code/life/life-server/node_modules/.pnpm/npm@9.6.5/node_modules/npm/node_modules:/Users/marcosm/code/life/life-server/node_modules/.pnpm/npm@9.6.5/node_modules:/Users/marcosm/code/life/life-server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/marcosm/code/life/life-server/node_modules/.pnpm/npm@9.6.5/node_modules/npm/bin/node_modules:/Users/marcosm/code/life/life-server/node_modules/.pnpm/npm@9.6.5/node_modules/npm/node_modules:/Users/marcosm/code/life/life-server/node_modules/.pnpm/npm@9.6.5/node_modules:/Users/marcosm/code/life/life-server/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../npm/bin/npm-cli.js" "$@"
else
  exec node  "$basedir/../npm/bin/npm-cli.js" "$@"
fi
