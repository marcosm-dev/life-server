{
  "name": "life-server",
  "version": "1.0.0",
  "description": "Servidor admin life template",
  "main": "index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "types": "/lib/typescript.d.ts",
  "scripts": {
    "lint": "eslint ./src/**/*",
    "dev": "npx tsc && node --watch ./dist/index.js",
    "build": "yarn clean && npx tsc -p ./tsconfig.json",
    "build:dev": "yarn clean && tsc && yarn copy-theme",
    "build:watch": "tsc -w",
    "clean": "rimraf dist",
    "start:dev": "node --watch --loader  ts-node/esm   ./src/index.ts",
    "watch": "node --watch --trace-warnings --loader  ts-node/esm   ./src/index.ts",
    "start": "npx tsc && node dist/index.js",
    "prepare": "husky install",
    "prettier-format": "prettier --config .prettierrc ./src/**/*.ts --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "parserOptions": {
      "project": "./ts.config.json"
    },
    "extends": [
      "./node_module/ts-standard/tsconfig.json"
    ]
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.0",
    "@types/compression": "1.7.2",
    "@types/cors": "2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "1.17.7",
    "@types/flat": "^5.0.2",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "20.5.1",
    "@types/nodemailer": "6.4.9",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "cross-env": "7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "7.33.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "onchange": "^7.1.0",
    "prettier": "3.0.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "ts-standard": "^12.0.2",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6",
    "volar-service-vetur": "latest"
  },
  "peerDependencies": {
    "adminjs": "^7.0.0"
  },
  "dependencies": {
    "@adminjs/design-system": "4.0.2",
    "@adminjs/express": "6.0.0",
    "@adminjs/mongoose": "4.0.0",
    "@adminjs/themes": "1.0.0",
    "@graphql-tools/schema": "^10.0.0",
    "adminjs": "^7.0.0",
    "argon2": "0.30.3",
    "axios": "1.4.0",
    "cloudinary": "^1.40.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "graphql": "^16.8.0",
    "graphql-type-json": "^0.3.2",
    "graphql-yoga": "4.0.4",
    "jsonwebtoken": "9.0.1",
    "mongoose": "^7.4.5",
    "nodemailer": "6.9.4",
    "react-feather": "2.0.10",
    "react-redux": "8.1.1",
    "rxjs": "7.8.1",
    "winston": "3.10.0"
  },
  "engines": {
    "node": ">=20.5.1 || 19.9.9"
  }
}
